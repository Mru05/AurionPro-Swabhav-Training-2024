package com.techlabs.model;

public class Facade {
	 private Game game;

	    public Facade() {
	        game = new Game();
	    }

	    public void startGame() {
	        game.play();
	    }
	}

	// Main class
	public class Game {
	    public static void main(String[] args) {
	        Facade facade = new Facade();
	        facade.startGame();

}
